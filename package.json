    "@hookform/resolvers": "^3.3.2",
    "@openfga/sdk": "^0.3.5",
    "auth0": "^4.28.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.294.0",
    "next": "^15.4.6",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "recharts": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5",
    "zod": "^3.22.4"
