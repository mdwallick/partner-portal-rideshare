  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "vercel-build": "prisma migrate deploy && prisma generate && next build",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
